('getState', this.showOptions)\n    },\n    getValue (name, value, index) {\n      console.log('name:', name)\n      this.selectValue = value\n      console.log('selectName:', this.selectName)\n      this.$emit('getValue', name, value, index, this.fieldName)\n    },\n    onBlur () {\n      this.showOptions = false\n      this.rotate = false\n      this.$emit('getState', this.showOptions)\n    }\n  }\n}\n</script>\n<style lang=\"less\" scoped>\n.m-select-box {\n  list-style: none;\n  width: 520px;\n  height: 50px;\n  background: transparent;\n  \n  .m-select-item {\n    width: 520px;\n    height: 50px;\n    position: relative;\n     input::-webkit-input-placeholder {\n              font-size: 14px; color: #999;\n            }\n            input:-moz-placeholder {\n              font-size: 14px; color: #999;\n            }\n            input::-moz-placeholder {\n              font-size: 14px; color: #999;\n            }\n            input:-ms-input-placeholder {\n              font-size: 14px; color: #999;\n            }\n    .u-select-value {\n        outline: none;\n      font-size: 16px;\n      width: 520px;\n      position: relative;\n      z-index: 999;\n      display: inline-block;\n      color: #333;\n      padding-top: 20px;\n      height: 30px;\n      line-height: 30px;\n      cursor: pointer;\n      border: none;\n      border-bottom: 1px solid #E3E3E3;\n    }\n    .s-has-error {\n      border-color: #dd0028;\n    }\n    .u-img1 {\n      width: 12px;\n      height: 12px;\n      cursor: pointer;\n      position: absolute;\n      z-index: 1000;\n      top: 24px;\n      right: 10px;\n      transition: transform 0.3s ease-in-out;\n    }\n    .u-img2 {\n      transform: rotate(180deg);\n    }\n    .m-options-box {\n      width: 100%;\n      position: absolute;\n      background: #FFFFFF;\n      border-radius: 8px;\n      border: 1px solid #E3E3E3;\n      top: 56px;\n      left: 0;\n      z-index: 999;\n      color: #333;\n      .m-options-item {\n        width: 100%;\n        height: 100%;\n        position: absolute;\n        top: 0;\n        left: 0;\n        z-index: 999;\n        .u-option {\n          margin: 0;\n          height: 40px;\n          line-height: 40px;\n          padding: 0 15px;\n          cursor: pointer;\n        }\n        .u-option:hover {\n          color: #0079DD;\n          background: #F2F4F6;\n        }\n      }\n    }\n  }\n}\n.show {\n  display: block;\n}\n.hidden {\n  display: none;\n}\n</style>\n"]}]}